git clone https://github.com/P3rf/rofi-network-manager.git /tmp/rofi-network-manager
cd /tmp/rofi-network-manager
cp rofi-network-manager.sh ~/.local/bin/rofi-network-manager
cp rofi-network-manager.conf ~/.local/bin/rofi-network-manager.conf
cp rofi-network-manager.rasi ~/.local/bin/rofi-network-manager.rasi
chmod +x ~/.local/bin/rofi-network-manager
